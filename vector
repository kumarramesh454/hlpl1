#include "std_lib_facilities.h"

int main()
{
	vector<int> v {1, 2, 3, 4, 5};

	v.push_back(4);
	v.push_back(4);
	

	cout << "Size of the vector: " << v.size()
		<< endl;

	for(int i = 0; i < v.size(); ++i)
		cout << v[i] << endl; // '\n'

	cout << "Other way to print elements:\n";

	for (int e : v)
		cout << e << endl;

	return 0;
}
